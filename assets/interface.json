    "interface_version": 2,
    "name": "MaaQKS",
    "description": "这是一个使用 MaaFramework 创建的新项目。",
    "contact": "QQ群: -",
    "license": "MIT",
    "welcome": "首次使用！欢迎使用 MAA!",
    // 填写项目地址和版本号可实现自动更新功能
    "github": "https://github.com/Aberstone/MaaQKS.git",
    "version": "1.0.0",
    // Mirror酱为第三方分发服务，新项目默认关闭
    // 若有需要请联系 Mirror酱 开通，直接自行开启是无法使用的
    // "mirrorchyan_rid": "MaaXXX",
    // "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720 // 默认缩放分辨率的短边长度
        },
        {
            "name": "TapTapPC端",
            "type": "Win32",
            "display_short_side": 720,
            "win32": {
                "window_regex": "风之痕迹",
                "mouse": "PostMessageWithCursorPos",
                "screencap": "FramePool"
            }
        }
    ],
    "resource": [
        {
            "name": "官服-EMU-1280*720",
            "path": [
                "./resource"
            ]
        },
        {
            "name": "TapTapPC端",
            "path": [
                "./resource"
            ]
        }
    ],
    "task": [
        {
            "name": "我要当古战场高手",
            "entry": "QKS_MoreGachaTicket",
            "description": "测试刷章鱼没啥问题，打完一管体力会尝试打一把章鱼妹"
        },
        {
            "name": "我要住在素材本",
            "entry": "Material_MoreMaterial",
            "description": "刷素材本，我喜欢刷风，所以默认刷风素材"
        },
        {
            "name": "我要住在使徒家",
            "entry": "Atreides_MoreOldenBattleFieldPoint",
            "option": [
                "使徒助战魔物选择"
            ]
        },
        {
            "name": "纯纯跳过工具人",
            "entry": "SkipBot_SkipBot",
            "description": "检测不到跳过按钮超过一定时长后会退出任务，默认值为20000ms",
            "option": [
                "timeout"
            ]
        },
        {
            "name": "大世界-抽卡房间切换",
            "entry": "WorldSwitch_SwitchBetweenWorldAndGachaRoom",
            "option": [
                "world_switch_screencap_rate"
            ]
        }
    ],
    "option": {
        "world_switch_screencap_rate": {
            "type": "input",
            "label": "$world_switch_screencap_rate",
            "inputs": [
                {
                    "name": "rate_limit",
                    "label": "$rate_limit",
                    "description": "默认值为1000ms, 按照自己机器的情况调整",
                    "default": "1000",
                    "pipeline_type": "int",
                    "verify": "^\\d+$"
                }
            ],
            "pipeline_override": {
                "WorldSwitch_MainRouter": {
                    "rate_limit": "{rate_limit}"
                }
            }
        },
        "timeout": {
            "type": "input",
            "label": "$timeout",
            "description": "检测不到跳过按钮超过一定时长后会退出任务，默认值为20000ms",
            "inputs": [
                {
                    "name": "timeout_ms",
                    "label": "$timeout_ms",
                    "description": "检测不到跳过按钮超过一定时长后会退出任务，默认值为20000ms",
                    "default": "20000",
                    "pipeline_type": "int",
                    "verify": "^\\d+$"
                }
            ],
            "pipeline_override": {
                "SkipBot_MainRouter": {
                    "timeout": "{timeout_ms}"
                }
            }
        },
